{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "data",
    "header",
    "url",
    "args",
    "files",
    "form",
    "json"
  ],
  "properties": {
    "data": {
      "enum": [
        "some data"
      ]
    },
    "url": {
      "enum": [
        "https://postman-echo.com/post"
      ]
    },
    "headers": {
      "type": "object",
      "required": [
        "x-forwarded-proto",
        "x-forwarded-port",
        "host",
        "x-amzn-trace-id",
        "content-length",
        "accept",
        "content-type",
        "user-agent",
        "accept-encoding"
      ],
      "properties": {
        "x-forwarded-proto": {
          "enum": [
            "https"
          ]
        },
        "x-forwarded-port": {
          "type": "string"
        },
        "x-amzn-trace-id": {
          "type": "string"
        },
        "host": {
          "enum": [
            "postman-echo.com"
          ]
        },
        "content-length": {
          "type": "string"
        },
        "accept": {
          "enum": [
            "*/*"
          ]
        },
        "content-type": {
          "type": "string"
        },
        "user-agent": {
          "type": "string"
        },
        "accept-encoding": {
          "enum": [
            "gzip,deflate"
          ]
        }
      }
    }
  }
}
